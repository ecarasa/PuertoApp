{
  "expo": {
    "name": "PuertoBuenosAires",
    "slug": "PuertoBuenosAires",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/puerto.1024px.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Puerto Buenos Aires App (en desarrollo)",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    }
  }
}
